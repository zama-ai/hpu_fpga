    glwe_dimension= {{GLWE_K}}
    polynomial_size= {{N}}
    lwe_modular_std_dev= 0.0
    glwe_modular_std_dev= 0.0
    pbs_base_log= {{PBS_B_W}}
    pbs_level= {{PBS_L}}
    ks_base_log= {{KS_B_W}}
